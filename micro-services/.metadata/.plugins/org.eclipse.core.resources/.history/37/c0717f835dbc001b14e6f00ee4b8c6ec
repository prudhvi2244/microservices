eureka:  client:    register-with-eureka: true    fetch-registry: true    service-url:      defaultZone:  http://localhost:8761/eureka

spring:
  cloud:
    gateway:
      routes:
      - id: order-service
        uri:  lb://ORDER-SERVICE
        predicates:        - Path=/order/**
      - id: payment-service
        uri:  lb://PAYMENT-SERVICE
        predicates:
        - Path=/payment/**
        